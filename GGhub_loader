local function SendNotification(title, text, duration)
    local StarterGui = game:GetService("StarterGui")

    pcall(function()
        StarterGui:SetCore("SendNotification", {
            Title = title or "GG hub",
            Text = text or "",
            Duration = duration or 2
        })
    end)
end

local GameScripts = {
    -- Prison Life
    [155615604] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_PrisonLife",
    -- Ninja Tycoon
    [221718525] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_NinjaTycoon",
    -- Just a single Obby
    [94618981088971] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_JustASingleObby",
    -- oMega Obby
    [8003084678] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_OMegaObby",
    -- Tennis Clash
    [85056532479796] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_TennisClash",
    -- Saber Simulator
    [3823781113] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_SaberSimulator",
    -- Just a Simulator
    [86434508615047] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_JustASingleSimulator",
    -- RIP Brainrot
    [100438336521838] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_RIPBrainrot",
    -- Dig To Earth's Core
    [81440632616906] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_DigToEarthsCore",
    -- Steal A Vehicle
    [132195536417465] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_StealAVehicle",
    -- 99 Nights In The Forest (Lobby)
    [79546208627805] = "notify",
    -- 99 Nights In The Forest (Game)
    [126509999114328] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_99NightsInTheForest",
    -- Cut Trees
    [135880624242201] = "https://raw.githubusercontent.com/dnzingg/Main/refs/heads/main/GGhub_CutTrees"
}

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local placeId = game.PlaceId
local scriptUrl = GameScripts[placeId]

local function SafeLoad(url)
    local success, response = pcall(function()
        return game:HttpGet(url)
    end)

    if success and response and response ~= "" then
        local loadFunc, loadErr = loadstring(response)
        if not loadFunc then
            warn("[GGHub] Error compiling script:", loadErr)
            return
        end

        local execSuccess, execErr = pcall(loadFunc)
        if not execSuccess then
            warn("[GGHub] Error executing script:", execErr)
        end
    else
        warn("[GGHub] Failed to fetch script from URL:", url)
    end
end

if scriptUrl == "notify" then
    SendNotification("GG hub", "Start a game first!", 3.5)
elseif scriptUrl then
    SafeLoad(scriptUrl)
else
    SendNotification("GG hub", "Game not supported!", 3.5)
    warn("[GGHub] Game not supported! PlaceId:", placeId)
end
