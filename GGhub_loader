local placeIdStr = tostring(game.PlaceId)
local scriptUrl = GameScripts[placeIdStr] or GameScripts[game.PlaceId]

local function SafeLoad(url)
    local ok, data = pcall(function()
        return game:HttpGet(url)
    end)

    if not ok then
        warn("[GGHub] Fetch error:", url)
        return
    end

    local fn, err = loadstring(data)
    if not fn then
        warn("[GGHub] Compile error:", err)
        return
    end

    local ok2, execErr = pcall(fn)
    if not ok2 then
        warn("[GGHub] Runtime error:", execErr)
    end
end

if scriptUrl == "notify" then
    print("[GGHub] Start a game first!")
elseif scriptUrl then
    SafeLoad(scriptUrl)
else
    warn("[GGHub] Game not supported! PlaceId:", game.PlaceId)
end
